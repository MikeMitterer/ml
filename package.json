{
    "name": "@mmit/ml",
    "version": "0.1.9",
    "author": "Mike Mitterer",
    "description": "Mix of Machine-Learning-Examples - visualized with VUE and TypeScript",
    "keywords": [
        "typescript vue ml machine learning"
    ],
    "private": true,
    "scripts": {
        "start": "vue-cli-service serve --mode development --open",
        "tc": "tsc --noEmit",
        "lint": "vue-cli-service lint",
        "test": "yarn run test:unit",
        "test:unit": "vue-cli-service test:unit tests/unit/.*",
        "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*",
        "build": "vue-cli-service build --mode development",
        "build:prod": "vue-cli-service build --mode production",
        "deploy": "yarn version --new-version patch"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeMitterer/ml"
    },
    "license": "MIT",
    "dependencies": {
        "@mmit/logging": "^0.1.5",
        "@tensorflow/tfjs": "^1.5.2",
        "plotly.js": "^1.52.2",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.3",
        "vue-property-decorator": "^8.4.0",
        "vue-router": "^3.1.5",
        "vuetify": "^2.2.12",
        "vuetify-loader": "^1.4.3",
        "vuex": "^3.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-typescript": "^7.8.3",
        "@mdi/font": "^4.9.95",
        "@types/jest": "25",
        "@types/plotly.js": "^1.44.28",
        "@vue/cli-plugin-typescript": "^4.2.2",
        "@vue/cli-plugin-unit-jest": "^4.2.2",
        "@vue/cli-service": "^4.2.2",
        "@vue/test-utils": "1.0.0-beta.31",
        "jest": "25",
        "jest-extended": "^0.11.5",
        "moment": "^2.24.0",
        "sass": "^1.25.0",
        "sass-loader": "8.0.2",
        "ts-jest": "25",
        "tslint": "^6.0.0",
        "typescript": "^3.7.5",
        "vue-cli-plugin-vuetify": "^2.0.5",
        "vue-jest": "^4.0.0-beta.2",
        "vue-template-compiler": "^2.6.11",
        "vuex-module-decorators": "^0.16.1"
    }
}
